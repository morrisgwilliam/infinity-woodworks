    "deploy": "npm run build && npm run s3:upload:build && npm run cloudfront:invalidate",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
