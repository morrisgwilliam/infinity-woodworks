  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "cloudfront:invalidate": "aws cloudfront create-invalidation --distribution-id E28G7QH1Q729MR --paths \"/*\" --region us-east-1 --profile Personal",
